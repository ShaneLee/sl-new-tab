{
  "manifest_version": 3,
  "name": "Shane Lee's New Tab",
  "description": "Shane Lee's New Tab",
  "permissions": ["storage"],
  "version": "1.0.0",
  "host_permissions": [
    "https://localhost:8080/*",
    "<all_urls>"
  ],
  "permissions": [
    "scripting",
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "chrome_url_overrides" : {
    "newtab": "template/index.html"
  }
}
